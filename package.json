{
  "name": "@huggydigital/huggy-datepicker",
  "description": "A Vue Datepicker Component modified from the https://github.com/mengxiong10/vue2-datepicker 3.10.2 project",
  "main": "index.js",
  "module": "index.esm.js",
  "alias": {
    "huggy-datepicker": "./src/index.js"
  },
  "files": [
    "/locale",
    "/index.*",
    "/scss"
  ],
  "version": "1.1.12",
  "scripts": {
    "dev": "parcel ./example/dev.html --open --port 2235",
    "build:index": "cross-env NODE_ENV=production rollup -c",
    "build:locale": "cross-env NODE_ENV=production rollup -c rollup.locale.config.js",
    "build:css": "sass --style=compressed --no-source-map src/style/index.scss index.css && postcss index.css --use autoprefixer -o index.css --no-map && cp -R src/style scss",
    "build": "npm run test && npm run clean && npm run build:index && npm run build:css && npm run build:locale",
    "clean": "rimraf scss locale index.*",
    "lint": "eslint src/**/*.{js,vue}",
    "deploy:build": "cross-env NODE_ENV=production parcel build ./example/index.html --public-url ./ --out-dir _site --no-source-maps --no-minify",
    "deploy": "bash build/gh-pages-deploy.sh",
    "format": "prettier --write src/**/*",
    "test": "npm run lint && jest",
    "cov": "jest --coverage --coverageReporters=text-lcov | coveralls",
    "version": "conventional-changelog -p angular -i CHANGELOG.md -s && git add CHANGELOG.md",
    "release": "bash build/git.sh && bash build/release.sh"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/huggydigital/huggy-datepicker.git"
  },
  "keywords": [
    "vue",
    "calendar",
    "datepicker",
    "datetimepicker"
  ],
  "publishConfig": {
    "access": "public"
  },
  "author": "jhonedarts",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/huggydigital/huggy-datepicker/issues"
  },
  "homepage": "https://github.com/huggydigital/huggy-datepicker#readme",
  "peerDependencies": {
    "vue": "^2.5.0"
  },
  "dependencies": {
    "date-format-parse": "^0.2.7"
  },
  "devDependencies": {
    "@babel/core": "^7.9.0",
    "@babel/plugin-transform-object-assign": "^7.2.0",
    "@babel/plugin-transform-runtime": "^7.6.2",
    "@babel/preset-env": "^7.5.5",
    "@babel/runtime": "^7.7.2",
    "@commitlint/cli": "^16.0.2",
    "@commitlint/config-conventional": "^8.2.0",
    "@vue/babel-helper-vue-jsx-merge-props": "^1.0.0",
    "@vue/babel-preset-jsx": "^1.1.2",
    "acorn": "^7.4.1",
    "autoprefixer": "^10.4.2",
    "babel-core": "^7.0.0-bridge.0",
    "babel-eslint": "^10.0.2",
    "babel-jest": "^27.4.6",
    "conventional-changelog-cli": "^2.2.2",
    "core-js": "^3.3.5",
    "cross-env": "^6.0.3",
    "date-fns": "^2.16.1",
    "eslint": "^8.7.0",
    "eslint-config-airbnb-base": "^14.0.0",
    "eslint-config-prettier": "^6.1.0",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-vue": "^7.20.0",
    "flush-promises": "^1.0.2",
    "highlight.js": "10.4.1",
    "husky": "^3.1.0",
    "jest": "^27.4.7",
    "jest-serializer-vue": "^2.0.2",
    "lint-staged": "^12.1.7",
    "marked": "^4.0.10",
    "parcel": "^2.2.1",
    "postcss-cli": "^9.1.0",
    "rimraf": "^3.0.0",
    "rollup": "^1.26.0",
    "rollup-plugin-auto-external": "^2.0.0",
    "rollup-plugin-babel": "^4.3.3",
    "rollup-plugin-commonjs": "^10.1.0",
    "rollup-plugin-node-resolve": "^5.2.0",
    "rollup-plugin-terser": "^7.0.2",
    "rollup-plugin-vue": "^6.0.0",
    "sass": "^1.48.0",
    "vue": "^2.6.14",
    "vue-hot-reload-api": "^2.3.3",
    "vue-jest": "^3.0.7",
    "vue-template-compiler": "^2.6.14",
    "yarn-audit-fix": "^9.0.7"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "vue"
    ],
    "transform": {
      "^.+\\.js$": "<rootDir>/node_modules/babel-jest",
      ".*\\.(vue)$": "<rootDir>/node_modules/vue-jest"
    },
    "snapshotSerializers": [
      "jest-serializer-vue"
    ],
    "moduleNameMapper": {
      "huggy-datepicker": "<rootDir>/src"
    },
    "coverageReporters": [
      "text",
      "text-summary"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "**/*.{js,vue}": [
      "eslint --fix",
      "prettier --write",
      "git add"
    ]
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie < 10"
  ]
}
